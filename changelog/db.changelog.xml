<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

  <!-- Include changelog files in execution order -->

  <!-- 1. Tables and Constraints -->
  <include file="changes/00-generated-commoditycode-tables-baseline.xml" relativeToChangelogFile="true"/>

  <!-- 2. Functions and Views - always last -->
  <include file="functions/functions.xml" relativeToChangelogFile="true"/>
  <include file="views/views.xml" relativeToChangelogFile="true"/>


</databaseChangeLog>